<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ch.awae.esgcal.fx.MenuController">
    <padding>
        <Insets left="50" right="50"/>
    </padding>
    <center>
        <VBox alignment="CENTER" spacing="50">
            <Button onAction="#onExport">Exportieren</Button>
            <Button onAction="#onPublish">Publizieren</Button>
            <Button onAction="#onUnpublish">Publikation wiederrufen</Button>
        </VBox>
    </center>

</BorderPane>
